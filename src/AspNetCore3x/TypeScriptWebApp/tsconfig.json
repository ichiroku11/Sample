{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"noEmitOnError": true,
		"noImplicitReturns": true,
		"module": "es2015",
		"moduleResolution": "node",
		"removeComments": false,
		"sourceMap": true,
		"strict": true,
		"target": "es2019"
	},
	"exclude": [
		"node_modules",
		"wwwroot"
	]
}
